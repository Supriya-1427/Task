// While loop
let i = 1;
while (i <= 5) {
    console.log(i);
    i++;
}

// Do-While
let num;
do {
    num = 3; // assume user input
    console.log("Number is", num);
} while (num < 1);

// For loop
for (let i = 1; i <= 5; i++) {
    if (i % 2 === 0) continue;
    console.log("Odd number:", i);
}

// Nested Loop
for (let i = 1; i <= 3; i++) {
    for (let j = 1; j <= 3; j++) {
        console.log(`${i} x ${j} = ${i * j}`);
    }
}
